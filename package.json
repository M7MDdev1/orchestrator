{
  "name": "orchestrator",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev:api": "ts-node-dev --respawn src/server.ts",
    "dev:worker": "ts-node-dev --respawn src/worker.ts",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "build": "tsc",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "start:api": "node dist/server.js",
    "start:worker": "node dist/worker.js",
    "migrate:generate": "npm run build && npx ts-node ./node_modules/typeorm/cli migration:generate ./src/migrations/AutoMigration -d src/data-source.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.12.2",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.27"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.9.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "@types/jest": "^29.5.3",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.12"
  }
